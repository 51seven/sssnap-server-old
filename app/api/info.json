{
  "swagger": "2.0",
  "info": {
    "title": "sssnap API",
    "description": "API for communication between the sssnap server and his clients",
    "version": "0.0.1"
  },
  "host": "localhost:3000",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json",
    "multipart/form-data"
  ],
  "produces": [
    "application/json"
  ],
  "security": [
    {
      "googleOAuth2": [ ]
    }
  ],
  "securityDefinitions": {
    "googleOAuth2": {
      "type": "oauth2",
      "scopes": {},
      "flow": "accessCode",
      "description": "OAuth2 Login with Google",
      "x-auth": "google",
      "authorizationUrl": "https://github.com/login/oauth/authorize",
      "tokenUrl": "https://github.com/login/oauth/access_token"
    }
  }
}